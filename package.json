{
  "name": "simplebrowser",
  "productName": "simplebrowser",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "buildwin": "electron-builder build --win portable",
    "buildmac": "electron-builder build --mac portable",
    "buildlinux": "electron-builder build --linux portable"
  },
  "keywords": [],
  "author": {
    "name": "brahma",
    "email": "nomailaddress@for.you"
  },
  "build": {
    "win": {
      "icon": "src/logo.png"
    }
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "simplebrowser"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.23.6",
    "cross-fetch": "^3.1.5",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "17.1.0"
  }
}
